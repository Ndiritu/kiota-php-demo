{
    "name": "ndiritu/kiota-php-demo",
    "description": "Demo Microsoft Graph SDK generated using Kiota",
    "type": "library",
    "authors": [
        {
            "name": "Philip Gichuhi",
            "email": "pgichuhi@microsoft.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "microsoft/microsoft-graph": "^2.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.16",
        "phpstan/phpstan": "^1.4.6"
    },
    "autoload": {
        "psr-4": {
            "Kiota\\Demo\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": false
        }
    }
}
