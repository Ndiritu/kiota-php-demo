{
    "name": "ndiritu/kiota-php-demo",
    "description": "Demo Microsoft Graph SDK generated using Kiota",
    "type": "library",
    "authors": [
        {
            "name": "Philip Gichuhi",
            "email": "pgichuhi@microsoft.com"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "../msgraph-sdk-php",
            "only": ["microsoft/microsoft-graph"],
            "options": {
                "symlink": false
            }
        },
        {
            "type": "path",
            "url": "../msgraph-sdk-php/msgraph-sdk-php-core",
            "only": ["microsoft/microsoft-graph-core"],
            "options": {
                "symlink": false
            }
        },
        {
            "type": "path",
            "url": "../msgraph-sdk-php/msgraph-sdk-php-core/kiota/abstractions/php",
            "only": ["microsoft/kiota-abstractions"],
            "options": {
                "symlink": false
            }
        },
        {
            "type": "path",
            "url": "../msgraph-sdk-php/msgraph-sdk-php-core/kiota/authentication/php/phpleague",
            "only": ["microsoft/kiota-authentication-phpleague"],
            "options": {
                "symlink": false
            }
        },
        {
            "type": "path",
            "url": "../msgraph-sdk-php/msgraph-sdk-php-core/kiota/http/php/guzzle",
            "only": ["microsoft/kiota-http-guzzle"],
            "options": {
                "symlink": false
            }
        },
        {
            "type": "path",
            "url": "../msgraph-sdk-php/msgraph-sdk-php-core/kiota/serialization/php/json",
            "only": ["microsoft/kiota-serialization-json"],
            "options": {
                "symlink": false
            }
        },
        {
            "type": "path",
            "url": "../msgraph-sdk-php/msgraph-sdk-php-core/kiota/serialization/php/text",
            "only": ["microsoft/kiota-serialization-text"],
            "options": {
                "symlink": false
            }
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "microsoft/microsoft-graph": "dev-feat/kiota-preview",
        "microsoft/microsoft-graph-core": "@dev",
        "microsoft/kiota-authentication-phpleague": "@dev",
        "microsoft/kiota-http-guzzle": "@dev",
        "microsoft/kiota-serialization-json": "@dev",
        "microsoft/kiota-serialization-text": "@dev",
        "microsoft/kiota-abstractions": "@dev"

    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.16",
        "phpstan/phpstan": "^1.4.6"
    },
    "autoload": {
        "psr-4": {
            "Kiota\\Demo\\": "src/"
        }
    }
}
