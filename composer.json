{
    "name": "ndiritu/kiota-php-demo",
    "description": "Demo Microsoft Graph SDK generated using Kiota",
    "type": "library",
    "authors": [
        {
            "name": "Philip Gichuhi",
            "email": "pgichuhi@microsoft.com"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "./kiota/abstractions/php",
            "only": ["microsoft/kiota-abstractions"],
            "options": {
                "symlink": false
            }
        },
        {
            "type": "path",
            "url": "./kiota/authentication/php/phpleague",
            "only": ["microsoft/kiota-authentication-phpleague"],
            "options": {
                "symlink": false
            }
        },
        {
            "type": "path",
            "url": "./kiota/http/php/guzzle",
            "only": ["microsoft/kiota-http-guzzle"],
            "options": {
                "symlink": false
            }
        },
        {
            "type": "path",
            "url": "./kiota/serialization/php/json",
            "only": ["microsoft/kiota-serialization-json"],
            "options": {
                "symlink": false
            }
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "microsoft/kiota-authentication-phpleague": "@dev",
        "microsoft/kiota-http-guzzle": "@dev",
        "microsoft/kiota-serialization-json": "@dev",
        "microsoft/kiota-abstractions": "@dev",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.16",
        "phpstan/phpstan": "^1.4.6"
    },
    "autoload": {
        "psr-4": {
            "Microsoft\\Graph\\": "src/"
        }
    }
}
